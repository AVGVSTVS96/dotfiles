# macOS
.DS_Store
.crush

# Graphite config (token stored in secrets.env, generated on restore)
**/graphite/user_config

# GitHub config
gh/.config/gh/hosts.yml

# LazyVim lock file
nvim/.config/nvim/lazy-lock.json

# Zed editor
zed/.config/zed/**
!zed/.config/zed/settings.json

# Tmux plugins
tmux/.config/tmux/plugins

# Age encryption keys
**/.config/sops/age/
**/sops/age/

# Decrypted secrets
**/secrets.env.dec
*.dec.env

# SSH keys (only encrypted versions should be committed)
ssh/.ssh/id_ed25519
ssh/.ssh/id_ed25519.dec
ssh/.ssh/*_rsa
ssh/.ssh/*_rsa.pub
ssh/.ssh/config.local

# Compiled binaries (source code is in src/)
scripts/.local/bin/kbtrack

# Fish shell auto-generated files (regenerate with `fisher update`)
fish/.config/fish/fish_variables
fish/.config/fish/themes/
fish/.config/fish/completions/fisher.fish
fish/.config/fish/completions/fzf_configure_bindings.fish

# Fish functions/ - ignore plugin-generated files, track custom ones
fish/.config/fish/functions/*
# (no custom function files yet - add !negation lines here if needed)

# Fish conf.d/ - ignore plugin-generated files, track custom ones
fish/.config/fish/conf.d/*
!fish/.config/fish/conf.d/01-secrets.fish
!fish/.config/fish/conf.d/functions.fish
